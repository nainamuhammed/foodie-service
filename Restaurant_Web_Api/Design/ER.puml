@startuml



entity "User" {
    ID : int <<PK>> <<AutoGenerated>>
    --
    Email : VARCHAR (30)
    Name : VARCHAR (30)
    ProfilePicture : VARCHAR (MAX)
    Dob : DateTime
    PhoneNumber : VARCHAR (15)
    Password: VARCHAR (20)
    IsLocationOn : bit
    IsNotificationOn : bit
    IsDeleted : bit
    CreatedOn : DateTime
    UpdatedOn : DateTime
}

@enduml